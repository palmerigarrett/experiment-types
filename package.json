{
  "name": "shypes",
  "version": "0.0.25",
  "description": "A library meant to be used as a single source of truth for typescript types between client and server applications.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": {
          "types": "./types/index.d.ts",
          "default": "./esm/index.js"
      },
      "require": {
          "types": "./types/index.d.ts",
          "default": "./cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "remove-build": "rimraf types && rimraf esm && rimraf cjs",
    "build": "yarn remove-build && rollup -c --bundleConfigAsCjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pp": "yarn build && npm publish",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "files": [
    "cjs/*",
    "esm/*",
    "types/*",
    "README.md"
  ],
  "private": false,
  "sideEffects": false,
  "keywords": [],
  "author": "Garrett Palmeri <palmerigarrett@gmail.com>",
  "license": "UNLICENSED",
  "browser": "",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@open-wc/building-rollup": "^3.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.4.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "deepmerge": "^4.3.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@rollup/plugin-eslint": "^9.0.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.2"
  }
}
